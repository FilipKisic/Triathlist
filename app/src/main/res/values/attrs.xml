<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwimInfoCard">
        <attr name="day_of_week" format="string" />
        <attr name="lap_count" format="string" />
        <attr name="time" format="string" />
        <attr name="distance" format="string" />
    </declare-styleable>
    <declare-styleable name="SessionGoalCard">
        <attr name="unit_text_color" format="color" />
        <attr name="unit" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="ActionButton">
        <attr name="background_color" format="reference" />
        <attr name="button_text" format="string" />
    </declare-styleable>
    <declare-styleable name="InfoTextView">
        <attr name="placeholder" format="string" />
    </declare-styleable>
    <declare-styleable name="SessionCard">
        <attr name="activity_icon" format="reference" />
        <attr name="activity_title" format="string" />
        <attr name="title_text_color" format="reference" />
    </declare-styleable>
</resources>